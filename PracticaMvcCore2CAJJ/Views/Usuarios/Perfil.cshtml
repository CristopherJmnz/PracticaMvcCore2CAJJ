@using System.Security.Claims
<div class="container py-3">
    <div class="container">
        <h1>Bienvenido @Context.User.Identity.Name</h1>
        <p>Email: @Context.User.FindFirst(ClaimTypes.Email).Value</p>
        <p>Email: @Context.User.FindFirst(ClaimTypes.Surname).Value</p>
        <img height="150" width="150" src="~/images/@Context.User.FindFirst("FOTO").Value"/>
    </div>
</div>